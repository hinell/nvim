# https://editorconfig.org/

root                      = true
[*]
end_of_line               = lf
insert_final_newline      = true
trim_trailing_whitespace  = true
charset                   = utf-8

[.config/nvim/lua/**/*.lua]
indent_style              = tab
# For space indent style
#indent_size               = 4
tab_width                 = 4
